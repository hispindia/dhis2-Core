    	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.1.xsd
    	http://www.springframework.org/schema/task http://www.springframework.org/schema/task/spring-task.xsd
	    http://www.springframework.org/schema/batch http://www.springframework.org/schema/batch/spring-batch-2.2.xsd
    	http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.2.xsd
	    http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.2.xsd
	    http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">



	<!--  for Scheduled Task Auto Approve -->
   	
   	
   	<task:scheduled-tasks scheduler="scheduleAutoApproveTrackerData">
        <task:scheduled ref="org.hisp.dhis.reporting.action.scheduletask.ScheduleAutoApproveTrackerData" method="execute" cron="0 0 16 * * ?"/>
        <!--<task:scheduled ref="org.hisp.dhis.reporting.action.scheduletask.ScheduleAutoApproveTrackerData" method="execute" cron="0 0/5 * * * ?"/>-->
   	</task:scheduled-tasks>
	
	<!-- 0 0 0/1 1/1 * ? schedule  every day every hour -->
	<!-- value="0 0 22 * * ?" // for 10 PM daily -->
	<!-- value="0 0/30 * * * ?" // for every 30 minute -->
	<!-- cron="0 0/3 * * * ?" // every 3 min -->
	
	<task:scheduler id="scheduleAutoApproveTrackerData" pool-size="1"/>
	<bean id="org.hisp.dhis.reporting.action.scheduletask.ScheduleAutoApproveTrackerData"
		class="org.hisp.dhis.reporting.action.scheduletask.ScheduleAutoApproveTrackerData"
		scope="prototype">		
	</bean>


	<!--  for Scheduled Task Auto Approve End -->













